#!/bin/bash

##bias correct main files using the LN distribution
sitelist=$1
dist_opt=1

for locname in $(cat $sitelist)
do
#echo "########## $locname ############"
rm -f  out/"$locname".month.bc
./bias_correct quantiles_"$locname".month.obs quantiles_"$locname".month.vic in/"$locname".month out/"$locname".month.bc  $dist_opt 12

rm -f out/"$locname".year.bc
./bias_correct quantiles_"$locname".year.obs quantiles_"$locname".year.vic in/"$locname".year out/"$locname".year.bc $dist_opt 1

done
